	"name": "pousada-oasis",
	"main": "src/worker.ts",
	"compatibility_date": "2025-09-24",
	"compatibility_flags": [
		"global_fetch_strictly_public"
	],
		"assets": {
			"directory": "./dist"
		},
	"kv_namespaces": [
		{ "binding": "USERS_KV", "id": "73ab71e6562f495abcb22eeab181afb4" },
